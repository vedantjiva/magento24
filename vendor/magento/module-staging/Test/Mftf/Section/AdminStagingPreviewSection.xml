<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminStagingPreviewSection">
        <element name="calendarBtn" type="button" selector="div.staging-preview-item-title.staging-preview-item-calendar" timeout="30"/>
        <element name="calendarBtnActive" type="button" selector="div.staging-preview-item-title.staging-preview-item-calendar._active" timeout="30"/>
        <element name="previewCalendarBtn" type="button" selector="div[class*='staging-preview-item-calendar']+div button[data-role='update-button']" timeout="30"/>
        <element name="shareBtn" type="button" selector="button[data-role='share-button']" timeout="30"/>
        <element name="shareUrl" type="input" selector="input[name='url']"/>
        <element name="category" type="text" selector="//span[text()='{{categoryName}}']" parameterized="true" timeout="20" />
        <element name="stagingDate" type="input" selector="#staging_date" />
        <element name="previewBtn" type="button" selector="[data-role='update-button']" timeout="30"/>
        <element name="previewScopeBtn" type="button" selector="div[class*='staging-preview-item-customer']+div button[data-role='update-button']" timeout="30"/>
        <element name="scopeBtn" type="button" selector=".staging-preview-item-customer" timeout="30"/>
        <element name="scopeBtnActive" type="button" selector=".staging-preview-item-customer._active"/>
        <element name="storeView" type="select" selector=".staging-preview-item-segment select"/>
        <element name="previewDate" type="text" selector=".staging-preview-options .subtitle"/>
    </section>
</sections>
