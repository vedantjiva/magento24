<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Support:etc/report.xsd">
    <group name="general" priority="10">
        <title translate="true">General</title>
        <sections>
            <section model="Magento\Support\Model\Report\Group\General\VersionSection" priority="40" />
            <section model="Magento\Support\Model\Report\Group\General\DataCountSection" priority="50" />
        </sections>
    </group>
    <group name="environment" priority="30">
        <title translate="true">Environment</title>
        <sections>
            <section model="Magento\Support\Model\Report\Group\Environment\EnvironmentSection" priority="410" />
        </sections>
    </group>
</config>
