                <item name="cms_block" xsi:type="string">Magento\Cms\Api\Data\BlockInterface</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Staging\Model\EntityStaging">
        <arguments>
            <argument name="stagingServices" xsi:type="array">
