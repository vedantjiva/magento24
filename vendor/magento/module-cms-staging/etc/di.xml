                <item name="Magento\Cms\Api\Data\BlockInterface" xsi:type="string">Magento\CmsStaging\Api\BlockStagingInterface</item>
            </argument>
        </arguments>
    </type>
    <type name="Magento\Framework\EntityManager\MetadataPool">
        <arguments>
