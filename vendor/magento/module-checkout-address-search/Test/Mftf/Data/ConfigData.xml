<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="DisableCustomerAddressSearch">
        <!-- Magento default value -->
        <data key="path">checkout/options/enable_address_search</data>
        <data key="label">No</data>
        <data key="value">0</data>
    </entity>
    <entity name="EnableCustomerAddressSearch">
        <data key="path">checkout/options/enable_address_search</data>
        <data key="label">Yes</data>
        <data key="value">1</data>
    </entity>
    <entity name="DisplayBillingAddressOnPaymentMethod">
        <!-- Magento default value -->
        <data key="path">checkout/options/display_billing_address_on</data>
        <data key="label">Payment Method</data>
        <data key="value">0</data>
    </entity>
    <entity name="DisplayBillingAddressOnPaymentPage">
        <data key="path">checkout/options/display_billing_address_on</data>
        <data key="label">Payment Page</data>
        <data key="value">1</data>
    </entity>
    <entity name="DefaultAddressSearchLimit">
        <!-- Magento default value -->
        <data key="path">checkout/options/customer_address_limit</data>
        <data key="label">10</data>
        <data key="value">10</data>
    </entity>
    <entity name="AddressSearchLimitZero">
        <data key="path">checkout/options/customer_address_limit</data>
        <data key="label">0</data>
        <data key="value">0</data>
    </entity>
    <entity name="AddressSearchLimitTwo">
        <data key="path">checkout/options/customer_address_limit</data>
        <data key="label">2</data>
        <data key="value">2</data>
    </entity>
    <entity name="AddressSearchLimitThree">
        <data key="path">checkout/options/customer_address_limit</data>
        <data key="label">3</data>
        <data key="value">3</data>
    </entity>
</entities>
