<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Order by SKU</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\AdvancedCheckout\Block\Customer\Sku" name="checkout.customer.sku" template="Magento_AdvancedCheckout::widget/sku.phtml">
                <action method="setIsTitleHidden">
                    <argument name="value" xsi:type="string">true</argument>
                </action>
                <block class="Magento\Framework\View\Element\Template" name="checkout.customer.sku.file" template="Magento_AdvancedCheckout::customer/file.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>
