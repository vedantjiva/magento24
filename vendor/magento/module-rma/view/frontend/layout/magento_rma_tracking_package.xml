<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="root">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">popup.phtml</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Rma\Block\Returns\Tracking\Package" name="rma.tracking.popup" template="Magento_Rma::return/popup/package.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
