<operations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataOperation.xsd">
    <operation name="RmaSettingsSetup" dataType="magento_rma" type="create" auth="adminFormKey" url="/admin/system_config/save/section/sales/" method="POST">
        <object key="groups" dataType="magento_rma">
            <object key="magento_rma" dataType="magento_rma">
                <object key="fields" dataType="magento_rma">
                    <object key="enabled" dataType="enabled">
                        <field key="value">string</field>
                    </object>
                    <object key="enabled_on_product" dataType="enabled_on_product">
                        <field key="value">string</field>
                    </object>
                    <object key="use_store_address" dataType="use_store_address">
                        <field key="value">string</field>
                    </object>
                </object>
            </object>
        </object>
    </operation>
</operations>