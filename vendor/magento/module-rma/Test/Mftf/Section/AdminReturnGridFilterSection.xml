<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="AdminReturnGridFilterSection">
        <element name="search" type="button" selector="#rmaGrid .admin__filter-actions [data-action='grid-filter-apply']" timeout="30"/>
