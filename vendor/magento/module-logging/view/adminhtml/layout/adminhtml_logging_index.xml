<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="formkey"/>
    <update handle="adminhtml_logging_block"/>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Logging\Block\Adminhtml\Container" name="adminhtml.logging.grid.container" template="Magento_Backend::widget/grid/container.phtml">
                <arguments>
                    <argument name="header_text" xsi:type="string">Admin Actions Log</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
