<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="giftregistry_page_head_components" template="Magento_GiftRegistry::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\GiftRegistry\Block\Customer\Checkout\Multishipping" name="giftregistry.customer.checkout" template="Magento_GiftRegistry::customer/checkout/multishipping.phtml" cacheable="false"/>
        </referenceContainer>
        <referenceBlock name="additional.product.info">
            <block class="Magento\GiftRegistry\Block\Cart\Product\Mark" name="giftregistry.product.mark" template="Magento_GiftRegistry::cart/product/mark.phtml"/>
        </referenceBlock>
    </body>
</page>
