<page layout="2columns-left" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="page_calendar"/>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="giftregistry_page_head_components" template="Magento_GiftRegistry::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\GiftRegistry\Block\Search\Form" name="giftregistry.search.form" template="Magento_GiftRegistry::search/form.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
