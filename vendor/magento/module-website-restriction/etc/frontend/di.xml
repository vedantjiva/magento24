    </type>
    <type name="Magento\WebsiteRestriction\Observer\RestrictWebsite">
        <arguments>
            <argument name="restrictor" xsi:type="object">Magento\WebsiteRestriction\Model\Restrictor\Proxy</argument>
        </arguments>
    </type>
</config>
