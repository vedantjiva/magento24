<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AdminDisableMultipleWishlistActionGroup">
        <annotations>
            <description>Disables the Multiple Wishlist via the CLI.</description>
        </annotations>
        <magentoCLI command="config:set {{multipleWishlist.path}} {{defaultMultipleWishlist.value}}"
                    stepKey="disableMultipleWishlist"/>
    </actionGroup>
</actionGroups>
