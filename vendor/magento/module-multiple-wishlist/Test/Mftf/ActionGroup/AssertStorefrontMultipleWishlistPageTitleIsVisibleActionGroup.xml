<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="AssertStorefrontMultipleWishlistPageTitleIsVisibleActionGroup">
        <annotations>
            <description>Check that the Multiple Wishlist page title is visible</description>
        </annotations>
        <seeElement selector="{{StorefrontMultipleWishlistManagementSection.multipleWishlistPageTitle}}"
                    stepKey="seeMultipleWishlistTitle"/>
    </actionGroup>
</actionGroups>
