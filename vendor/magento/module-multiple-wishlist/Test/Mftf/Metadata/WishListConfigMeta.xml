<operations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="urn:magento:mftf:DataGenerator/etc/dataOperation.xsd">
    <operation name="EnableMultipleWishListConfigState" dataType="wishlist_config_state" type="create" auth="adminFormKey" url="/admin/system_config/save/section/wishlist/" method="POST">
        <object key="groups" dataType="wishlist_config_state">
            <object key="general" dataType="wishlist_config_state">
                <object key="fields" dataType="wishlist_config_state">
                    <object key="active" dataType="active">
                        <field key="value">string</field>
                    </object>
                    <object key="multiple_enabled" dataType="multiple_enabled">
                        <field key="value">string</field>
                    </object>
                    <object key="multiple_wishlist_number" dataType="multiple_wishlist_number">
                        <field key="value">string</field>
                    </object>
                </object>
            </object>
        </object>
    </operation>
</operations>

